{
    "extends": "eslint:recommended",
    "parser": "babel-eslint",
    "env": {
        "es6": true,
        "browser": true,
        "jest/globals": true
    },
    "parserOptions": {
        "ecmaFeatures": {
            "modules": true
        }
    },
    "plugins": ["jest"],
    "globals": {
        "window": true
    },
    "rules": {
        "semi": [2, "never"]
    }
}
